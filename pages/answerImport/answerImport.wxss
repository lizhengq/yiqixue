page{background:#F2F2F2;}
.banner{position: fixed;left:0;top:0;right:0;height:422rpx;background:#ccc;z-index: 10;}
.main{position:absolute;top:422rpx;left:0;right:0;bottom:0;z-index:-1;}
.main .main-row{border-bottom:1px solid #C8C7CC;}
.main .main-row .p{box-sizing: border-box;display:block;width:100%;text-align: center;font-size:28rpx;color:#949FB3;padding:40rpx 30rpx;}
.main .main-row textarea{display:block;width:100%;box-sizing: border-box;height:220rpx;background:#fff;font-size:28rpx;color:#333;padding:30rpx;}
.submit{padding:79rpx 80rpx 50rpx 80rpx;}
.submit text{display:block;width:100%;height:88rpx;line-height:88rpx;background:#B8DD98;color:#fff;text-align: center;border-radius:50rpx;}