page{background:#F0F0F0;}
.scrolls{position: absolute;left:0;top:0;right:0;bottom:300rpx;}


.li .li-title{color:#212121;font-size:80rpx;text-align: center;font-weight: bold;margin-bottom:20rpx;}
.li .li-fy{display:flex;flex-direction: row;align-items:center;justify-content: center;margin-bottom:105rpx;}
.li .icons{position: absolute;z-index: 8;display: block;width:60rpx;height:60rpx;top:38rpx;right:30rpx;}
.li .succ{background:url('http://amcimg.vipyqx.com/commFile/oss/image/download/a55b067874dd4dd8b4c1265d75c029dc.png') no-repeat;background-size:contain;}
.li .err{background:url('http://amcimg.vipyqx.com/commFile/oss/image/download/38a5b99847d74639963221ddd6f9da18.png') no-repeat;background-size:contain;}
.li .li-fy .icon-voice{display:block;width:60rpx;height:60rpx;background:url('http://amcimg.vipyqx.com/commFile/oss/image/download/15dca959382142a8be38b5493f4094b7.png') no-repeat;background-size:contain;margin-right:15rpx;}

.li{padding:0 30rpx 0 30rpx;}
.li .li-row{position: relative;background:#fff;margin-bottom:30rpx;border-radius: 16rpx;padding:30rpx;height:136rpx;}
.li .li-row text{display:inline-block;font-size:28rpx;color:#636A77;}
.bottom-wrap{position:fixed;z-index: 9;left:0;right:0;bottom:60rpx;padding:0 30rpx 0 59rpx;}
.bottom-wrap .pages{display:inline-block;font-size:36rpx;color:#212121;}
.bottom-wrap .b-l{color:#999;margin-top:10rpx;}
.bottom-wrap .see{width:200rpx;height:66rpx;border-radius:8rpx;border:1px solid #DBDBDB;text-align: center;line-height:66rpx;font-size:28rpx;color:#636A77;}
.loadings{position: fixed;z-index: 9;top:0;right:0;bottom:0;left:0;text-align: center;padding:200rpx 0 0 0;}
.loadings .l-bg{display:block;width:475rpx;height:436rpx;background:url('http://amcimg.vipyqx.com/commFile/oss/image/download/295c860c87af4b7c8fa21a73797726ca.png') no-repeat;background-size:contain;margin:0 auto;}
.loadings .l-bottom{position: absolute;bottom:100rpx;left:0;right:0;}
.loadings text{display:block;font-size:36rpx;color:#636A77;line-height:1;padding-bottom:30rpx;}
.loadings .progress{width:606rpx;height:24rpx;background:#DCDCDC;border-radius: 12rpx;overflow: hidden;position: relative;margin:0 auto;}
.loadings .pro{position: absolute;left:0;top:0;background:#7EC83F;width:20%;height: 100%;}